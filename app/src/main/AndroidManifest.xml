<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.notificationmoduledesign">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".MainActivities.MainActivityP11"
            android:exported="false" />
        <activity
            android:name=".MainActivities.MainActivityP5"
            android:exported="false" />
        <activity
            android:name=".MainActivities.MainActivityP10"
            android:exported="false" />
        <activity
            android:name=".MainActivities.MainActivityP3"
            android:exported="false" />
        <activity
            android:name=".MainActivities.MainActivityP8"
            android:exported="false" />
        <activity
            android:name=".MainActivities.MainActivityP12"
            android:exported="false" />
        <activity
            android:name=".MainActivities.MainActivityP2"
            android:exported="false" />
        <activity
            android:name=".MainActivities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>